<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterController</servlet-name>
        <servlet-class>Controller.RegisterController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FinalUpdate</servlet-name>
        <servlet-class>Controller.FinalUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusController</servlet-name>
        <servlet-class>Controller.BusController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BusUpdate</servlet-name>
        <servlet-class>Controller.BusUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Controller.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormController</servlet-name>
        <servlet-class>Controller.FormController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FBusUpdate</servlet-name>
        <servlet-class>Controller.FBusUpdate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterController</servlet-name>
        <url-pattern>/RegisterController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FinalUpdate</servlet-name>
        <url-pattern>/FinalUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusController</servlet-name>
        <url-pattern>/BusController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BusUpdate</servlet-name>
        <url-pattern>/BusUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormController</servlet-name>
        <url-pattern>/FormController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FBusUpdate</servlet-name>
        <url-pattern>/FBusUpdate</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
